{
  "name": "github-issue-glance",
  "displayName": "Github Issue Glance",
  "version": "1.0.1",
  "author": "qwadrox",
  "description": "Helps you bookmark and manage GitHub issues with ease",
  "type": "module",
  "license": "Unlicense",
  "keywords": [
    "github chrome extension",
    "github issues",
    "bookmark issues",
    "github issue organizer",
    "github issue glance"
  ],
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "fmt": "prettier --write '**/*.{ts,json,css,scss,md}'",
    "format-check": "prettier --check '**/*.{ts,json,css,scss,md}'",
    "zip": "npm run build && node src/zip.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta.19",
    "@types/chrome": "^0.0.246",
    "@types/jest": "^29.5.14",
    "gulp": "^4.0.2",
    "gulp-zip": "^6.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.2.5",
    "typescript": "^5.2.2",
    "vite": "^4.4.11"
  },
  "dependencies": {
    "ag-grid-community": "^33.0.3"
  }
}